{
  "name": "snippod-webapp-boilerplate",
  "description": "Snippod frontend webapp boilerplate using react redux and hot reloading",
  "author": "Seonggyu Lee <shalomeir@gmail.com> (http://github.com/shalomeir)",
  "license": "MIT",
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "homepage": "http://www.shalomeir.com",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "hot reloading",
    "react-hot-reloader",
    "redux",
    "starter",
    "boilerplate",
    "babel",
    "snippod"
  ],
  "main": "excute/server.js",
  "scripts": {
    "start": "concurrent --kill-others \"npm run start-prod\"",
    "notused_start": "concurrent --kill-others \"npm run start-prod\" \"npm run start-prod-api\"",
    "start-prod": "node ./node_modules/better-npm-run start-prod",
    "notused_start-prod-api": "node ./node_modules/better-npm-run start-prod-api",
    "build": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "build:i18n": "rimraf _translations && babel --quiet --babelrc .babelrc.i18n src > /dev/null",
    "build:i18n:langs": "npm run build:i18n && babel-node scripts/i18nToXliff.js",
    "postinstall": "webpack --display-error-details --config webpack/prod.config.js",
    "lint": "eslint -c .eslintrc src api",
    "start-dev": "node ./node_modules/better-npm-run start-dev -- --nw",
    "notused_start-dev-api": "node ./node_modules/better-npm-run start-dev-api",
    "watch-client": "node ./node_modules/better-npm-run watch-client",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "notused_dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\" \"npm run start-dev-api\"",
    "test": "karma start"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./excute/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 3000,
        "APIPORT": 8000
      }
    },
    "start-prod-api": {
      "command": "node ./excute/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "production",
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "node ./excute/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 8000
      }
    },
    "start-dev-api": {
      "command": "node ./excute/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development",
        "APIPORT": 3030
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src"
      }
    }
  },
  "dependencies": {
    "babel": "5.8.23",
    "babel-plugin-typecheck": "1.3.0",
    "body-parser": "1.14.1",
    "compression": "1.6.0",
    "cookies-js": "^1.2.2",
    "express": "4.13.3",
    "express-session": "1.11.3",
    "file-loader": "0.8.4",
    "history": "1.12.5",
    "http-proxy": "1.11.2",
    "humps": "^1.0.0",
    "less": "2.5.3",
    "less-loader": "2.2.1",
    "lodash": "^3.10.1",
    "lru-memoize": "1.0.0",
    "map-props": "1.0.0",
    "multireducer": "1.0.2",
    "normalizr": "^1.3.1",
    "piping": "0.3.0",
    "pretty-error": "1.2.0",
    "query-string": "3.0.0",
    "react": "^0.14.0",
    "react-document-meta": "2.0.0-rc2",
    "react-dom": "0.14.0",
    "react-inline-css": "2.0.0",
    "react-intl": "2.0.0-pr-3",
    "react-redux": "^4.0.0",
    "react-router": "1.0.0-rc3",
    "redux": "3.0.1",
    "redux-form": "^2.2.6",
    "serialize-javascript": "1.1.2",
    "serve-favicon": "2.3.0",
    "serve-static": "1.10.0",
    "shallowequal": "0.2.2",
    "socket.io": "1.3.7",
    "socket.io-client": "1.3.7",
    "superagent": "1.4.0",
    "url-loader": "0.5.6",
    "webpack-isomorphic-tools": "0.9.3"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-intl": "^1.0.0-beta-5",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-runtime": "^5.8.25",
    "better-npm-run": "^0.0.2",
    "bootstrap-sass": "^3.3.5",
    "bootstrap-sass-loader": "^1.0.9",
    "chai": "^3.3.0",
    "clean-webpack-plugin": "^0.1.3",
    "concurrently": "^0.1.1",
    "css-loader": "^0.19.0",
    "eslint": "1.7.1",
    "eslint-config-airbnb": "0.1.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-import": "^0.8.0",
    "eslint-plugin-react": "^3.5.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "font-awesome": "^4.4.0",
    "font-awesome-webpack": "0.0.4",
    "glob": "^5.0.15",
    "mkdirp": "^0.5.1",
    "json-loader": "^0.5.3",
    "karma": "^0.13.10",
    "karma-chrome-launcher": "^0.2.0",
    "karma-cli": "^0.1.1",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.3",
    "node-sass": "^3.3.3",
    "react-a11y": "^0.2.6",
    "react-addons-test-utils": "^0.14.0",
    "react-hot-loader": "^1.3.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "redux-router": "1.0.0-beta3",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.0.0",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.2",
